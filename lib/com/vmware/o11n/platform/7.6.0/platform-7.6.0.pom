    <profiles>
        <profile>
            <id>jdk-8</id>
            <activation>
               <jdk>[1.8,1.9)</jdk>
            </activation>
            <properties>
                <additionalparam>-Xdoclint:none</additionalparam>
            </properties>
        </profile>
        <profile>
            <id>integration-test</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <!-- only available here -->
                        <inherited>false</inherited>
                        <executions>
                            <execution>
                                <id>clean-integration-installation</id>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <phase>clean</phase>
                                <configuration>
                                    <tasks if="o11n.root.path">
                                        <delete dir="${o11n.root.path}" />
                                    </tasks>
                                </configuration>
                            </execution>
                            <execution>
                                <id>initialize-integration-installation</id>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <phase>initialize</phase>
                                <configuration>
                                    <tasks if="o11n.root.path">
                                        <mkdir dir="${o11n.root.path}" />
                                    </tasks>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <!-- profile specifically used for cleaning the installation 
                folder prior to building. It does the same as integration-test profile, but 
                I'm not sure what exactly does the integration-test profile do additionally 
                cleaning the installation folder. -->
            <id>clean-build</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-antrun-plugin</artifactId>
                        <!-- only available here -->
                        <inherited>false</inherited>
                        <executions>
                            <execution>
                                <id>clean-integration-installation</id>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <phase>clean</phase>
                                <configuration>
                                    <tasks if="o11n.root.path">
                                        <delete dir="${o11n.root.path}" />
                                    </tasks>
                                </configuration>
                            </execution>
                            <execution>
                                <id>initialize-integration-installation</id>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <phase>initialize</phase>
                                <configuration>
                                    <tasks if="o11n.root.path">
                                        <mkdir dir="${o11n.root.path}" />
                                    </tasks>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
		</profile>
		<profile>
			<id>gerrit</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.jacoco</groupId>
						<artifactId>jacoco-maven-plugin</artifactId>
						<version>${jacocoVersion}</version>
						<executions>
						   <execution>
								<id>jacoco-prepare</id>
								<goals>
									<goal>prepare-agent</goal>
								</goals>
							</execution>
							<execution>
								<id>jacoco-report</id>
								<phase>prepare-package</phase>
								<goals>
									<goal>report</goal>
								</goals>
								<configuration>
									<outputDirectory>${project.build.directory}/jacoco</outputDirectory>
								    <skip>${jacocoSkipReport}</skip>
								</configuration>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>findbugs</id>
			<build>
				<plugins>
                    <plugin>
                        <groupId>com.github.spotbugs</groupId>
                        <artifactId>spotbugs-maven-plugin</artifactId>
                        <version>${maven.plugin.spotbugs}</version>
                        <configuration>
                            <threshold>Normal</threshold>
                            <effort>Default</effort>
                            <failOnError>false</failOnError>
                            <xmlOutput>true</xmlOutput>
                            <omitVisitors>UnreadFields,DefaultEncodingDetector</omitVisitors>
                            <excludeFilterFile>${findbugs_exclude}</excludeFilterFile>
                        </configuration>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>check</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
				</plugins>
			</build>
		</profile>
    </profiles>
