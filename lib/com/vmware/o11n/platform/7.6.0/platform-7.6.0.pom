                    <version>2.3.1</version>
                    <configuration>
                        <archive>
                            <manifestEntries>
                                <Build-Number>${build.number}</Build-Number>
                                <Specification-Vendor>VMware, Inc.</Specification-Vendor>
                                <App-Version>${project.version}</App-Version>
                                <App-Vendor-Short>vmware</App-Vendor-Short>
                                <Built-By>vmware</Built-By>
                            </manifestEntries>
